{
  "name": "multi-currency-backend",
  "version": "0.0.1",
  "description": "A multicurrency wallet backend and authentication",
  "scripts": {
    "dev": "nodemon --exec babel-node --max-old-space-size=8192 src/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "rm -rf dist && babel src --out-dir dist --copy-files",
    "standard-version": "git add . && standard-version --commit-all",
    "pre-release:minor": "git add . && standard-version --commit-all --release-as minor",
    "pre-release:patch": "git add . && standard-version --commit-all --release-as patch",
    "pre-release:major": "git add . && standard-version --commit-all --release-as major",
    "release": "npm-run-all build standard-version",
    "release:minor": "npm-run-all build pre-release:minor",
    "release:patch": "npm-run-all build pre-release:patch",
    "release:major": "npm-run-all build pre-release:major",
    "lint": "eslint src",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js"
  },
  "keywords": [
    "MultiCurrency",
    "Crypto",
    "Runebase",
    "RUNES",
    "Pirate",
    "ARRR",
    "Tokel",
    "TKL",
    "Stellar",
    "Lumens",
    "XLM",
    "DogeLumens",
    "DXLM"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobpepers/multi-currency-backend"
  },
  "author": "Bago",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-async-generator-functions": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-security": "^1.5.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.4.1",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "algebraic-captcha": "^1.0.1",
    "axios": "^0.27.2",
    "bcrypt-nodejs": "^0.0.3",
    "bignumber.js": "^9.0.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "compression": "^1.6.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.7.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "nodemailer-express-handlebars": "^5.0.0",
    "otpauth": "^7.1.3",
    "p-queue": "^6.6.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.0",
    "rate-limiter-flexible": "^2.3.7",
    "recaptcha-v2": "^0.1.3",
    "redis": "^4.1.0",
    "sequelize": "^6.20.1",
    "sharp": "^0.30.6",
    "socket.io": "^4.5.1",
    "stellar-sdk": "^10.1.2",
    "svg-captcha": "^1.4.0",
    "utf8": "^3.0.0",
    "winston": "^3.7.2"
  }
}
